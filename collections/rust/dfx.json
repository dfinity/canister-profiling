{
  "version": 1,
  "canisters": {
    "hashmap": {
      "type": "custom",
      "candid": "collection.did",
      "wasm": "target/wasm32-unknown-unknown/release/hashmap_opt.wasm",
      "build": 
         ["cargo build --target wasm32-unknown-unknown --release --package hashmap",
          "ic-wasm target/wasm32-unknown-unknown/release/hashmap.wasm -o target/wasm32-unknown-unknown/release/hashmap_opt.wasm shrink"
         ]
    },
    "btreemap": {
      "type": "custom",
      "candid": "collection.did",
      "wasm": "target/wasm32-unknown-unknown/release/btreemap_opt.wasm",
      "build":
         ["cargo build --target wasm32-unknown-unknown --release --package btreemap",
          "ic-wasm target/wasm32-unknown-unknown/release/btreemap.wasm -o target/wasm32-unknown-unknown/release/btreemap_opt.wasm shrink"
         ]
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  }
}
